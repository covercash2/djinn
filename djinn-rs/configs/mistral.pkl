abstract class Mistral {
	fixed name: String
	fixed variant: "Mistral"|"QMistral"
	/// Enable tracing
	tracing: Boolean
	flashAttention: Boolean
	seed: Number
	sampleLength: Number(isPositive)
	/// Penalty to be applied for repeating tokens, 1. means no penalty
	repeatPenalty: Number
	/// The context size to consider for the repeat penalty
	repeatLastN: Number(isPositive)
}

class QMistral extends Mistral {
	fixed name: "QMistral"
	fixed variant: "QMistral"
}

name: String = "Mistral"

variant: "Mistral"|"QMistral" = "QMistral"

seed: Number = 299792458

sampleLength: Number = 100

/// Penalty to be applied for repeating tokens, 1. means no penalty
repeatPenalty: Number = 1.1

repeatLastN: Number = 64
